<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="CitizenPortal" version="2.5">

	<display-name>PrimeFaces</display-name>
	<welcome-file-list>
		<welcome-file>index.xhtml</welcome-file>
	</welcome-file-list>

	<context-param>
		<param-name>com.sun.faces.enableRestoreView11Compatibility</param-name>
		<param-value>true</param-value>
	</context-param>

	<context-param>
		<param-name>primefaces.THEME</param-name>

		<!-- <param-value>bootstrap</param-value> -->

		<!-- <param-value>aristo</param-value> -->

		<param-value>blitzer</param-value>

	</context-param>

	<context-param>
		<param-name>javax.faces.STATE_SAVING_METHOD</param-name>
		<param-value>client</param-value>
	</context-param>

	<servlet>
		<servlet-name>Faces Servlet</servlet-name>
		<servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>Faces Servlet</servlet-name>
		<url-pattern>*.jsf</url-pattern>
		<url-pattern>*.xhtml</url-pattern>
	</servlet-mapping>

	<context-param>
		<param-name>javax.faces.FACELETS_SKIP_COMMENTS</param-name>
		<param-value>true</param-value>
	</context-param>

	<!-- Private key for Morse is 6LeLzfYSAAAAAOw2OPSvG7LnQ5TVj6lMjDIL3aKa Public 
		key for Morse is 6LeLzfYSAAAAAHR_VCbI4ua484S5g_e7VS10FsYO -->
	<context-param>
		<param-name>primefaces.PRIVATE_CAPTCHA_KEY</param-name>
		<param-value>6LdBmfYSAAAAALt154H-BC0N79M3kgwFQ9lEitGG</param-value>
	</context-param>

	<context-param>
		<param-name>primefaces.PUBLIC_CAPTCHA_KEY</param-name>
		<param-value>6LdBmfYSAAAAAJgweR4gTUNEivjwQzHL7kC6ycRF</param-value>
	</context-param>

	<!-- Session Configuration -->
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>

</web-app>